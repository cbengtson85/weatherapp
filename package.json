{
  "name": "weatherapp",
  "version": "1.0.0",
  "description": "weather app",
  "main": "./server/server.js",
  "moduleRoots": [
    "."
  ],
  "scripts": {
    "test": "cross-env NODE_PATH=. mocha --compilers js:babel-core/register -r test/setup.js test/**",
    "lintNode": "eslint -c .eslintrc-node.json server/** config/**",
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js app/** test/**",
    "preclean": "echo '\\033[0;36m BEGIN cleaning /dist directory \\033[0m'",
    "clean": "find dist -not -name 'dist' -not -name '.gitignore' | xargs rm -rf",
    "postclean": "echo '\\033[1;36m DONE cleaning /dist directory \\033[0m'",
    "predevbuild": "echo '\\033[0;36m BEGIN building dev\\033[0m' && npm run clean",
    "devbuild": "webpack --config config/webpack.config.js",
    "postdevbuild": "echo '\\033[1;32m SUCCESS building dev\\033[0m'",
    "devstart": "nodemon --watch server --watch config --watch app/views -e js,marko server/server.js --server dev",
    "devstartdocker": "nodemon --watch server --watch config --watch app/views -e js,marko server/server.js --server dev --mongoHost weatherappdb",
    "devbuildwatch": "webpack --watch --config config/webpack.config.js",
    "devnodestart": "node server/server.js --server dev",
    "devnodestartdocker": "node server/server.js --server dev --mongoHost weatherappdb",
    "devwatch": "npm run clean; npm run devbuildwatch & npm run devstart",
    "devwatchdocker": "npm run devbuildwatch & npm run devstartdocker"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cbengtson85@bitbucket.org/cbengtson85/weatherapp.git"
  },
  "author": "cbengtson",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/cbengtson85/weatherapp/issues"
  },
  "homepage": "https://bitbucket.org/cbengtson85/weatherapp/overview",
  "dependencies": {
    "axios": "^0.15.1",
    "dateformat": "^2.0.0",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "marko": "^3.13.2",
    "minimist": "^1.2.0",
    "mongodb": "^2.2.10",
    "react": "^15.3.2",
    "react-addons-perf": "^15.4.1",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "react-slick": "^0.14.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  }
}
