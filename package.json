{
  "name": "weatherapp",
  "version": "1.0.0",
  "description": "weather app",
  "main": "./server/server.js",
  "scripts": {
    "preclean": "echo '\\033[0;36m BEGIN cleaning /dist directory \\033[0m'",
    "clean": "find dist -not -name 'dist' -not -name '.gitignore' | xargs rm -rf",
    "postclean": "echo '\\033[1;36m DONE cleaning /dist directory \\033[0m'",
    "predevbuild": "echo '\\033[0;36m BEGIN building dev\\033[0m' && npm run clean",
    "devbuild": "webpack --config config/webpack.config.js",
    "postdevbuild": "echo '\\033[1;32m SUCCESS building dev\\033[0m' && tput bel",
    "devdebug": "node-inspector & nodemon --debug --watch server --watch config server/server.js --server dev",
    "devstart": "nodemon --watch server --watch config --watch app/views -e js,marko server/server.js --server dev",
    "devbuildwatch": "webpack --watch --config config/webpack.config.js",
    "devnodestart": "node server/server.js --server dev",
    "devwatch": "npm run clean; npm run devbuildwatch & npm run devstart"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cbengtson85@bitbucket.org/cbengtson85/weatherapp.git"
  },
  "author": "cbengtson",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/cbengtson85/weatherapp/issues"
  },
  "homepage": "https://bitbucket.org/cbengtson85/weatherapp/overview",
  "dependencies": {
    "axios": "^0.11.1",
    "body-parser": "^1.15.0",
    "dateformat": "^1.0.12",
    "express": "^4.13.4",
    "jquery": "^3.0.0",
    "marko": "^3.2.0",
    "minimist": "^1.2.0",
    "mongodb": "^2.1.16",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-inspector": "^0.12.8",
    "nodemon": "^1.9.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0"
  }
}
